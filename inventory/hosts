## host1 ansible_host=127.0.0.1 ansible_user=vagrant ansible_port=2222 ansible_connection=local
## host2 ansible_host=127.0.0.1 ansible_user=vagrant ansible_port=2200 ansible_connection=local
[host_a]
host1 ansible_host=127.0.0.1 ansible_user=vagrant ansible_port=2222 ansible_connection=local
[host_b]
host2 ansible_host=127.0.0.1 ansible_user=vagrant ansible_port=2200 ansible_connection=local

[host_a:vars]
 ansible_ssh_private_key_file=./.vagrant/machines/host1/virtualbox/private_key

[host_b:vars]
ansible_ssh_private_key_file=./.vagrant/machines/host2/virtualbox/private_key
